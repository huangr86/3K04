{
  "modes": ["AOO", "VOO", "AAI", "VVI", "AOOR", "VOOR", "AAIR", "VVIR"],


  "schema": {
    "LRL_ppm":            { "label":"               Lower Rate Limit","unit":"ppm","type":"int",
                            "ranges" : [   { "min": 30, "max": 50, "inc": 5 },  { "min": 50, "max": 90, "inc": 1}, { "min": 90, "max": 175, "inc": 5} ],
                            "modes": ["AOO", "VOO", "AAI","VVI", "AOOR", "AAIR", "VOOR", "VVIR"] },

    "URL_ppm":            {"label":"Upper Rate Limit","unit":"ppm","type":"int",
                            "ranges" : [   { "min": 50, "max": 175, "inc": 5 }],

                            "modes": ["AOO", "VOO", "AAI","VVI", "AOOR", "AAIR", "VOOR", "VVIR"] },
    "Atrial_Amp_V":       {"label":"Atrial Amplitude","unit":"V","type":"float",
                            "ranges" : [   { "min": 0.5, "max": 3.2, "inc": 0.1 }, { "min": 3.5, "max": 7.0, "inc": 0.5 }],
                            "modes": ["AOO", "AAI", "AOOR", "AAIR"]},

    "Ventricular_Amp_V":  {"label":"Ventricular Amplitude","unit":"V","type":"float",
                            "ranges" : [   { "min": 0.5, "max": 3.2, "inc": 0.1 }, { "min": 3.5, "max": 7.0, "inc": 0.5 }],
                            "modes": ["VOO", "VVI", "VOOR", "VVIR"] },

    "Atrial_PW_ms":       {"label":"Atrial Pulse Width","unit":"ms","type":"float",
                            "ranges" : [   { "min": 1, "max": 30, "inc": 1 }],
                            "modes": ["AOO", "AAI", "AOOR", "AAIR"]},

    "Atrial Sensitivity":  {"label":"Atrial Sensitivity","unit":"mv","type":"float",
                            "ranges" : [   { "min": 1, "max": 10, "inc": 0.5 }, { "min": 0.25, "max": 0.75, "inc": 0.25 }],
                            "modes": ["AAI", "AAIR"] },

    "Ventricular Sensitivity":  {"label":"Ventricular Sensitivity","unit":"mv","type":"float",
                            "ranges" : [   { "min": 1, "max": 10, "inc": 0.5 }, { "min": 0.25, "max": 0.75, "inc": 0.25 }],
                            "modes": ["VVI", "VVIR"] },

    "Ventricular_PW_ms":  {"label":"Ventricular Pulse Width","unit":"ms","type":"float",
                            "ranges" : [   { "min": 1, "max": 30, "inc": 1 }],
                            "modes": ["VOO", "VVI", "VOOR", "VVIR"] },  

    "VRP_ms":             {"label":"VRP (Ventricular Refr. P.)","unit":"ms","type":"int",
                            "ranges" : [   { "min": 150, "max": 500, "inc": 10 }],
                            "modes": ["VVI", "VVIR"] },

    "ARP_ms":             {"label":"ARP (Atrial Refr. P.)","unit":"ms","type":"int",
                            "ranges" : [   { "min": 150, "max": 500, "inc": 10 }],
                            "modes": ["AAI", "AAIR"] },

    "Rate Smoothing":     {"label":"Rate Smoothing","unit":"%","type":"int",
                            "ranges" : [   { "min": 0, "max": 21, "inc": 3}, { "min": 25, "max": 25, "inc": 1}],
                            "modes":  ["AAI","VVI", "AAIR", "VVIR"] },

    "Reaction Time":     {"label":"Reaction Time","unit":"s","type":"int",
                            "ranges" : [   { "min": 10, "max": 50, "inc": 10}],
                            "modes":  ["AAOR", "AAIR", "VOOR", "VVIR"] },

    "Response Factor":     {"label":"Response Factor","unit":"","type":"int",
                            "ranges" : [   { "min": 1, "max": 16, "inc": 1}],
                            "modes":  ["AAOR", "AAIR", "VOOR", "VVIR"] },

    "Recovery Time":     {"label":"Recovery Time","unit":"min","type":"int",
                            "ranges" : [   { "min": 2, "max": 16, "inc": 1}],
                            "modes":  ["AAOR", "AAIR", "VOOR", "VVIR"] },

    "Activity Threshold":     {"label":"Activity Threshold", "allowed": ["V-Low", "Low", "Med-Low", "Med", "Med-High", "High", "V-High"],
                            "modes":  ["AAOR", "AAIR", "VOOR", "VVIR"] },

    "Hysteresis":     {"label":"Hysteresis", "allowed": ["Off", "Track LRL"],
                            "modes":  ["AAI","VVI", "AAIR", "VVIR"] }





  },
  "defaults": {
    "LRL_ppm": 60,
    "URL_ppm": 120,
    "Atrial_Amp_V": 3.5,
    "Atrial_PW_ms": 1,
    "Ventricular_Amp_V": 3.5,
    "Ventricular_PW_ms": 1,
    "VRP_ms": 320,
    "ARP_ms": 250,
    "Atrial Sensitivity": 0.75,
    "Ventricular Sensitivity": 2.5,
    "Rate Smoothing": 0,
    "Reaction Time" : 30,
    "Response Factor" : 8,
    "Recovery Time" : 5,
    "Activity Threshold" : "Med",
    "Hysteresis" : "Off" 

  },
  "descriptions": {
    "LRL_ppm": "Minimum pacing rate allowed.",
    "URL_ppm": "Maximum pacing rate allowed.",
    "Atrial_Amp_V": "Voltage of atrial pacing pulse.",
    "Atrial_PW_ms": "Duration of atrial pacing pulse.",
    "Ventricular_Amp_V": "Voltage of ventricular pacing pulse.",
    "Ventricular_PW_ms": "Duration of ventricular pacing pulse.",
    "VRP_ms": "After a ventricular beat, briefly ignore new ventricular blips.",
    "ARP_ms": "After an atrial beat, briefly ignore new atrial blips.",
    "Atrial Sensitivity": "Determines how small an atrial signal the pacemaker will detect.",
    "Ventricular Sensitivity": "Determines how small an ventricular signal the pacemaker will detect.",
    "Rate Smoothing": "limit the pacing rate change",
    "Reaction Time": "How quickly the pacemaker speeds up your heart rate when you become active",
    "Response Factor": "Determines how strongly the pacemaker adjusts the heart rate in response to patient activity",
    "Recovery Time": "the time it takes for the heart rate to drop from the maximum safe rate (MSR)",
    "Activity Threshold": "add stuff",
    "Hysteresis": "add stuff"
  }
}
